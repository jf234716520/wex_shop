<!--miniprogram/pages/XyApprove/XyApprove.wxml-->
<!-- 选项卡 -->
        <view class="approve-box">
          <!-- 商品卡片 -->
          <view class="approve-list" wx:for="{{approve}}" wx:key="{{index}}">

            <image class="approve-thumb" src="{{item.good_img}}"></image> 小图
            <text class="approve-pro-name">{{item.good_name}}</text> 用户名
            <text class="approve-pro-price">￥{{item.good_price}}</text> 额度
          </view>

</view>